   OBJECTS { fullLogLine }
   STATUS current
   DESCRIPTION
      "The monotonic system clock went backwards"
   ::= { trapsZero 231 }

clockjump NOTIFICATION-TYPE
   OBJECTS { fullLogLine }
   STATUS current
   DESCRIPTION
      "The system clock jumped forwards or backwards by more than one second"
   ::= { trapsZero 232 }

