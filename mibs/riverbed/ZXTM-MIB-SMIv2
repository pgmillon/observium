      nodeFailures         Counter32,
      nodeNewConn          Counter32,
      nodeErrors           Counter32,
      nodeResponseMin      Gauge32,
      nodeResponseMax      Gauge32,
      nodeResponseMean     Gauge32,
      nodeCurrentConn      Gauge32
   }

