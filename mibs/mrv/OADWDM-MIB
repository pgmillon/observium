oaLdRoadmPortThresholdIndex OBJECT-TYPE
        SYNTAX  INTEGER
        {
         degradeLow             (1),
         degradeHigh            (2),
         failLow                (3),
         failHigh               (4),
         hysteresisDegradeLow   (5),
         hysteresisDegradeHigh  (6),
         hysteresisFailLow      (7),
         hysteresisFailHigh     (8),
         tRaise                 (9),
         tFall                  (10)
        }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
