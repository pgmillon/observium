        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The ROADM module Channel Power, 
         (Drop Peak Power for module EM1600-WSS9, 
         Com Output West Total Power for module EM1600-WSS2Q,
         Com Output Total Power for module EM1600-WSS4D)
         Units: 0.1 [dBm]"
        ::= { oaLdRoadmChannelEntry 8 }

