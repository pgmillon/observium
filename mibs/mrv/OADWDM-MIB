        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
        "The slot#. An index from 1 to The number of slots"
        ::= { oaLdAdcCardPortsInfoEntry 1 }

oaLdAdcCardPortsPortNumber OBJECT-TYPE
        SYNTAX    INTEGER
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION
