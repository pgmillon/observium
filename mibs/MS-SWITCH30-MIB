syslogDestFacility OBJECT-TYPE
	SYNTAX INTEGER
	{
	kernelMessage         (0),
	userLevelMessage      (1),
	mailSystem            (2),
	systemDaemon          (3),
	securityMessage1      (4),
	syslogdMessage        (5),
	linePrinterSubsystem  (6),
	networkNewsSubsystem  (7),
