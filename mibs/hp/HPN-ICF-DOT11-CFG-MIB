        "Each entry contains information of each radio in AP."
    INDEX
        {
            hpnicfDot11RadioIfIdx
        }
    ::= { hpnicfDot11RadioIntfConfigTable 1 }

HpnicfDot11RadioIntfConfigEntry ::= SEQUENCE
