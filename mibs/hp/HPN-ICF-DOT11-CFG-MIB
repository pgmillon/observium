    DEFVAL      { 60 }
    ::= { hpnicfDot11ServicePolicyEntry 21 }

hpnicfDot11AuthenModeCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            opensystem(0),
            sharedkey(1)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
