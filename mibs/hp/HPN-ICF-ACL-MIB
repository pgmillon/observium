                hpnicfPfilterRunApplyDirection,
                hpnicfPfilterRunApplyAclType,
                hpnicfPfilterRunApplyAclIndex,
                hpnicfPfilterAclRuleIndex
            }
            ::= { hpnicfPfilterAclRuleRunInfoTable 1 }

        HpnicfPfilterAclRuleRunInfoEntry ::=
        SEQUENCE
        {
