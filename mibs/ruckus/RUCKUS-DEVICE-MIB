    SYNTAX  OCTET STRING  (SIZE (2..40))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Specifies forth trap destination IPv6 or IPv4 address."
    ::= { ruckusDeviceTrapInfo 4 }

ruckusDeviceTrapDestination5 OBJECT-TYPE
    SYNTAX  OCTET STRING  (SIZE (2..40))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Specifies fifth trap destination IPv6 or IPv4 address."
    ::= { ruckusDeviceTrapInfo 5 }

