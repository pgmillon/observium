                    Unsigned32,
                cpeConfigMgrDownloadBasePath
                    OCTET STRING,
                cpeConfigMgrDownloadSecureMode
                    INTEGER,
                cpeConfigMgrTrapsEnabled
                    TruthValue,
                cpeConfigScriptIndex
                    Unsigned32,
                cpeConfigVarsIndex
                    Unsigned32
             }

