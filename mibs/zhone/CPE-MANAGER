            SYNTAX Unsigned32
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "Unique number used to identify this download server instance.  
                This field is the table index."
            ::= { cpeConfigMgrDownloadServerEntry 2 }

        
