-- The Community Table determines access to the device. Incoming
-- SNMP requests are first validated for their community name and
-- their access right determined. An optional additional level
-- of security is provided by allowing specification of only certain
-- IP addresses. This can be specified on a per community name
-- basis. This function is provided by the use of an access list
-- (i.e. IP address list) table whose entries can be used by 
-- multiple community name tables.
-- 
		-- 1.3.6.1.4.1.5504.3.1.4.1
		zhoneAdminCommunityTable OBJECT-TYPE
			SYNTAX SEQUENCE OF ZhoneAdminCommunityEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Zhone proprietary Community Table."
			::= { zhoneAdmin 1 }

		
		-- 1.3.6.1.4.1.5504.3.1.4.1.1
		zhoneAdminCommunityEntry OBJECT-TYPE
			SYNTAX ZhoneAdminCommunityEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A community name access to this system."
			INDEX { IMPLIED zhoneAdminCommunityName }
			::= { zhoneAdminCommunityTable 1 }

		
		ZhoneAdminCommunityEntry ::=
			SEQUENCE { 
				zhoneAdminCommunityName
					ZhoneAdminString,
				zhoneAdminCommunityAccess
					INTEGER,
				zhoneAdminCommunityAccessListIndex
					Integer32,
				zhoneAdminCommunityRowStatus
					ZhoneRowStatus
			 }

-- ZHONE_KEYWORD_PROFILE_LABEL = community-name
-- ZHONE_KEYWORD_STORAGE_LABEL = Community_Profile.name
-- ZHONE_KEYWORD_QUICK_ACCESS  = TRUE
-- ZHONE_KEYWORD_STORAGE_ORDER = 1
-- 
		-- 1.3.6.1.4.1.5504.3.1.4.1.1.1
		zhoneAdminCommunityName OBJECT-TYPE
			SYNTAX ZhoneAdminString (SIZE (1..32))
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The community name that has access to this system.
				
				Maximum length for community name string is 32 characters."
			::= { zhoneAdminCommunityEntry 1 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = permissions
-- ZHONE_KEYWORD_STORAGE_LABEL = Community_Profile.access_level
-- ZHONE_KEYWORD_STORAGE_ORDER = 2
-- 
		-- 1.3.6.1.4.1.5504.3.1.4.1.1.2
		zhoneAdminCommunityAccess OBJECT-TYPE
			SYNTAX INTEGER
				{
				noAccess(1),
				read(2),
				readAndWrite(3),
				admin(4)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The Access Rights for this particular community name. An access of read-write-admin
				allows access to those tables with a higher level of security such as the
				community table. Gets to this table will return E_NOSUCHINSTANCE unless
				the accessrights is read-write-admin. 
				
				noAccess - this community name does not have any access to this device.
				read - allows read access to all fields except admin access fields such
				            as this community table.
				readAndWrite - allow read and write access to all fields except admin access
				            fields such as this community table.
				admin - read and write access to all fields on this device."
			DEFVAL { read }
			::= { zhoneAdminCommunityEntry 2 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = access-table-index
-- ZHONE_KEYWORD_STORAGE_LABEL = Community_Profile.access_index
-- ZHONE_KEYWORD_QUICK_ACCESS  = TRUE
-- ZHONE_KEYWORD_STORAGE_ORDER = 3
-- 
		-- 1.3.6.1.4.1.5504.3.1.4.1.1.3
		zhoneAdminCommunityAccessListIndex OBJECT-TYPE
			SYNTAX Integer32 (0..50)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The AccessList Table index indicating which IP addresses are
				allowed to use this community name. If the value is
				0, no address checking is performed and all SNMP requests are
				allowed with the access right as specified in this table."
			DEFVAL { '0'b }
			::= { zhoneAdminCommunityEntry 3 }

		
		-- 1.3.6.1.4.1.5504.3.1.4.1.1.4
		zhoneAdminCommunityRowStatus OBJECT-TYPE
			SYNTAX ZhoneRowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Row status in order to create the entry for this 
				destination."
			::= { zhoneAdminCommunityEntry 4 }

		
-- The access list table is a proprietary table defined by 
-- Zhone for the use of defining which IP addresses are
-- allowed to send SNMP requests. The community name
-- table references, if applicable, an access list index
-- which along with any defined IP address indexes define
-- the total list of acceptable IP addresses. See the community
-- name table for further description.
-- 
		-- 1.3.6.1.4.1.5504.3.1.4.2
		zhoneNextAdminAccessIndex OBJECT-TYPE
			SYNTAX Integer32 (1..50)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The next available zhoneAdminAccessListIndex that can 
				be used by the manager when creating a access list
				entry.  A Get request to this field causes the current 
				number to be returned and the value to be incremented
				for the next requester.
				
				Once this value reaches 50, processing examines the
				entire list for possible free entries and provides the 
				lowest number available. If no entries are free, an
				error is returned.
				"
			::= { zhoneAdmin 2 }

		
		-- 1.3.6.1.4.1.5504.3.1.4.3
		zhoneAdminAccessListTable OBJECT-TYPE
			SYNTAX SEQUENCE OF ZhoneAdminAccessListEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Access List Table Definition."
			::= { zhoneAdmin 3 }

		
		-- 1.3.6.1.4.1.5504.3.1.4.3.1
		zhoneAdminAccessListEntry OBJECT-TYPE
			SYNTAX ZhoneAdminAccessListEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Zhone Access List Entry Definition."
			INDEX { zhoneAdminAccessListIndex, zhoneAdminAccessListIpAddress }
			::= { zhoneAdminAccessListTable 1 }

		
		ZhoneAdminAccessListEntry ::=
			SEQUENCE { 
				zhoneAdminAccessListIndex
					Integer32,
				zhoneAdminAccessListIpAddress
					IpAddress,
				zhoneAdminAccessRowStatus
					ZhoneRowStatus
			 }

-- ZHONE_KEYWORD_PROFILE_LABEL = access-table-index
-- ZHONE_KEYWORD_STORAGE_LABEL = Community_Access_Profile.index
-- ZHONE_KEYWORD_QUICK_ACCESS  = TRUE
-- ZHONE_KEYWORD_STORAGE_ORDER = 1
-- 
		-- 1.3.6.1.4.1.5504.3.1.4.3.1.1
		zhoneAdminAccessListIndex OBJECT-TYPE
