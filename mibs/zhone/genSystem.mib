--
-- Node definitions
--
	
		-- 1.3.6.1.4.1.5504.3.1.1
		zhoneZms OBJECT-IDENTITY
			STATUS current
			DESCRIPTION 
				"The following variables are associated with the state of
				existence and connectivity with the Zhone Management System
				(ZMS)."
			::= { zhoneSystem 1 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = zmsexists
-- ZHONE_KEYWORD_STORAGE_LABEL = System_Profile.zmsexists
-- ZHONE_KEYWORD_STORAGE_ORDER = 6
-- 
		-- 1.3.6.1.4.1.5504.3.1.1.1
		zhoneZmsExists OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Identifies whether or not this device is being managed by the Zhone
				Management System. This is only settable by the management system and
				NOT by the CLI as it indicates existence of the manager. Different
				operations are performed based on this setting (i.e. no configuration
				synchronization processing is performed if there is no Zhone Management
				System).
				
				Default value is 'false'."
			::= { zhoneZms 1 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = zmsconnectionstatus
-- ZHONE_KEYWORD_STORAGE_LABEL = System_Profile.zmsconnectionstatus
-- ZHONE_KEYWORD_STORAGE_ORDER = 7
-- 
		-- 1.3.6.1.4.1.5504.3.1.1.2
		zhoneZmsConnectionStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				active(1),
				inactive(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The status of the connection between this device and the Zhone 
				Management System, if one exists. This value can either be set 
				directly by ZMS or by the device internally when a SNMP request
				is processed from the ZMS. For devices that are being proxied by 
				another entity, since they are not being polled by ZMS, will 
				set this state according to the communication channel state
				between this device and the connected entity.
				
				Default value is 'inactive'."
			::= { zhoneZms 2 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = zmsipaddress
-- ZHONE_KEYWORD_STORAGE_LABEL = System_Profile.zmsipaddress
-- ZHONE_KEYWORD_STORAGE_ORDER = 8
-- 
		-- 1.3.6.1.4.1.5504.3.1.1.3
		zhoneZmsIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This is the ZMS IP address for which configuration synchronization 
				messages and any other special handling is performed. This can
				be set either via CLI or ZMS."
			::= { zhoneZms 3 }

		
		-- 1.3.6.1.4.1.5504.3.1.1.4
		zhoneZmsBlockCli OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"If TRUE, CLI only allows 'show' commands and cannot
				modify/add any data records. This is used by ZMS
				to prevent race conditions and to allow only single
				access to system resources especially during 
				problem analysis times.
				
				Default value is 'false'."
			::= { zhoneZms 4 }

		
-- The Zhone Trap information is information used to provide a 
-- reliable trap mechanism between the device and the Zhone
-- Management System. There are two trap flows utilized between the 
-- device and any fault management system. The first is used for most 
-- traps generated by the device where an incremental sequence number
-- is used. It is then up to the FMS to indicated any traps that
-- it missed asking for retransmission starting at a particular
-- sequence number. The other type is an Acknowledged trap where
-- only one trap will be outstanding at one time. This will
-- be a separate sequence number than the unacknowledged flow.
-- 
		-- 1.3.6.1.4.1.5504.3.1.2
		zhoneTraps OBJECT-IDENTITY
			STATUS current
			DESCRIPTION 
				"This is the trap information related to
				handling device to ZMS traps. "
			::= { zhoneSystem 2 }

		
-- The Table defining the list of trap destinations and associated trap 
-- message exchange handling.
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1
		zhoneTrapsTable OBJECT-TYPE
			SYNTAX SEQUENCE OF ZhoneTrapsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Zhone Traps Table
				"
			::= { zhoneTraps 1 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.1.1
		zhoneTrapsEntry OBJECT-TYPE
			SYNTAX ZhoneTrapsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"An entry per trap destination."
			INDEX { zhoneTrapsDestination }
			::= { zhoneTrapsTable 1 }

		
		ZhoneTrapsEntry ::=
			SEQUENCE { 
				zhoneTrapsDestination
					IpAddress,
				zhoneTrapsCommunityName
					ZhoneAdminString,
				zhoneTrapsSequenceNumber
					Unsigned32,
				zhoneTrapsResendSequenceNumber
					Integer32,
				zhoneTrapsAckedSequenceNumber
					Unsigned32,
				zhoneTrapsSeverity
					INTEGER,
				zhoneTrapsAllowedCategories
					BITS,
				zhoneTrapsAdminStatus
					INTEGER,
				zhoneTrapsRowStatus
					ZhoneRowStatus,
				zhoneTrapsGatewayTrapServerAddress
					ZhoneAdminString
			 }

-- ZHONE_KEYWORD_PROFILE_LABEL = trapdestination
-- ZHONE_KEYWORD_STORAGE_LABEL = Trap_Destination.trapdestination
-- ZHONE_KEYWORD_STORAGE_ORDER = 1
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.1
		zhoneTrapsDestination OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The location to be send traps."
			::= { zhoneTrapsEntry 1 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = communityname
-- ZHONE_KEYWORD_STORAGE_LABEL = Trap_Destination.communityname
-- ZHONE_KEYWORD_STORAGE_ORDER = 2
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.2
		zhoneTrapsCommunityName OBJECT-TYPE
			SYNTAX ZhoneAdminString
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Community name to be sent in all traps sent to this destination. This 
				field is defaulted to NULL so a null string will be sent to the
				destination."
			::= { zhoneTrapsEntry 2 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.3
		zhoneTrapsSequenceNumber OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Next sequence number to be sent as part of a trap
				generated by the device. The sequence number has two uses. In
				one case,it is the sequence number of the non-acknowledge traps
				and has a range of 0-2147483647. While the acknowledge sequence
				number range is 2147483648-4294967295. This field wraps to 
				the appropriate '0' value (0 or 2147483648) for it's range."
			::= { zhoneTrapsEntry 3 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = resendseqno
-- ZHONE_KEYWORD_STORAGE_LABEL = Trap_Destination.resendseqno
-- ZHONE_KEYWORD_STORAGE_ORDER = 3
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.4
		zhoneTrapsResendSequenceNumber OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"This is set by the ZMS in order
				to have the device resend saved traps because one
				or more were lost in transmission. The device will 
				resend traps starting at this sequence number. Note
				that due to resource constraints not all traps may
				be available to be resent. The first trap resent will
				indicate that this is a resent and whether or not
				any traps were deleted/lost. 
				
				This is used only on the unacknowledge trap flow. This is
				the only indication of whether or not any traps have
				been acknowledged.
				
				The value returned is always 0 as setting this field
				only causes restarting of the traps with this number."
			DEFVAL { 0 }
			::= { zhoneTrapsEntry 4 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = ackedseqno
-- ZHONE_KEYWORD_STORAGE_LABEL = Trap_Destination.ackedseqno
-- ZHONE_KEYWORD_STORAGE_ORDER = 4
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.5
		zhoneTrapsAckedSequenceNumber OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Acknowledged trap sequence number. This field is set 
				by the ZMS in order to acknowledge a trap sent
				by the device with the Request Acknowledge flag
				set to YES. There will be only one trap will be 
				outstanding on this flow (acknowledged flow)at one time so
				this should be the last sequence number sent. If not, it
				is ignored."
			::= { zhoneTrapsEntry 5 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = traplevel
-- ZHONE_KEYWORD_STORAGE_LABEL = Trap_Destination.traplevel
-- ZHONE_KEYWORD_STORAGE_ORDER = 5
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.6
		zhoneTrapsSeverity OBJECT-TYPE
			SYNTAX INTEGER
				{
				critical(1),
				severe(2),
				moderate(3),
				low(4)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The Trap severity level which at this level or higher, traps will
				be sent to this destination."
			DEFVAL { low }
			::= { zhoneTrapsEntry 6 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = traptype
-- ZHONE_KEYWORD_STORAGE_LABEL = Trap_Destination.traptype
-- ZHONE_KEYWORD_STORAGE_ORDER = 6
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.7
		zhoneTrapsAllowedCategories OBJECT-TYPE
			SYNTAX BITS
				{
				catAdmin(0),
				catLine(1),
				catVoice(2),
				catIP(3),
				catRip(4),
				catRadio(5)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Array of allowed categories to be sent to this particular
				trap destination. This field is not currently implemented
				and is reserved for future use."
			::= { zhoneTrapsEntry 7 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = trapadminstatus
-- ZHONE_KEYWORD_STORAGE_LABEL = Trap_Destination.trapadminstatus
-- ZHONE_KEYWORD_STORAGE_ORDER = 7
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.8
		zhoneTrapsAdminStatus OBJECT-TYPE
			SYNTAX INTEGER
				{
				enabled(1),
				disabled(2)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Whether traps should be enabled or disabled for this particular
				IP address."
			DEFVAL { enabled }
			::= { zhoneTrapsEntry 8 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.9
		zhoneTrapsRowStatus OBJECT-TYPE
			SYNTAX ZhoneRowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Row status in order to create the entry for this 
				destination. There are no required fields on a create
				besides the obvious index of the ip address of the 
				destination."
			::= { zhoneTrapsEntry 9 }

		
-- ZHONE_KEYWORD_PROFILE_LABEL = gatewaytrapserveraddr
-- ZHONE_KEYWORD_STORAGE_LABEL = Trap_Destination.gatewaytrapserveraddr
-- ZHONE_KEYWORD_STORAGE_ORDER = 8
-- 
		-- 1.3.6.1.4.1.5504.3.1.2.1.1.10
		zhoneTrapsGatewayTrapServerAddress OBJECT-TYPE
			SYNTAX ZhoneAdminString
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The Gateway Trap Server Address is used to locate the Gateway.
				Trap Server. This field is defaulted 0,0,0."
			DEFVAL { "none" }
			::= { zhoneTrapsEntry 10 }

		
-- The following define common trap OIDs to be used in all traps sent
-- to the fault manager service.
--  
		-- 1.3.6.1.4.1.5504.3.1.2.2
		zhoneTrapVersion OBJECT-TYPE
			SYNTAX INTEGER { version1(1) }
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"Version number of the format of the trap. Any changes to
				the common area of a trap require a bump of this number."
			::= { zhoneTraps 2 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.3
		zhoneTrapFlags OBJECT-TYPE
			SYNTAX BITS
				{
				trapAcknowledge(0),
				trapOutOfSequence(1),
				trapResetSequenceNumber(2)
				}
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This variable is sent with all traps and contains flags related
				to this trap. 
				
				trapAcknolwedge: If this bit is set, it indicates to the ZMS 
				                 that the trap must be acknowledged.
				                 Acknowledgement is accomplished by the ZMS 
				                 sending a SET request to the variable 
				                 zhoneTrapsAckedSequenceNumber in the 
				                 zhoneTrapsTable. See the trap sequence number
				                 field description (value is 2147483648 and 
				                 higher.
				trapOutOfSequence: If set, it indicates that this trap is being
				                 sent out of sequence from what the trap
				                 receiver is expecting. The specific occurence is
				                 when the trap receiver detects lost traps and
				                 requests resending of traps starting from a
				                 specific sequence number (see 
				                 zhoneTrapsResendSequenceNumber) but the system
				                 does not have all these traps. Example:
				- traps 400-600 sent to the trap receiver.
				                 - trap 500 is lost
				                 - trap receiver requests resend of traps 500 and
				                   up.
				                 - system only has 550-600 saved in memory.
				                 - trap 550 resent with trapOutOfSequence set.
				                 - trap 551 and above resent with trapOutOfSequence
				                   reset.
				trapResetSequenceNumber: If set, the sender of this trap has
				reset the trap sequence number for this trap
				destination. It is indicating to the ZMS to
				also reset its sequence numbers."
			::= { zhoneTraps 3 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.4
		zhoneTrapShelf OBJECT-TYPE
			SYNTAX ZhoneShelfValue
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This variable is sent with all traps. This contains the
				value of the shelf that originated the trap."
			::= { zhoneTraps 4 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.5
		zhoneTrapSlot OBJECT-TYPE
			SYNTAX ZhoneSlotValue
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This variable is sent with all traps. This contains the
				value of the slot that originated the trap."
			::= { zhoneTraps 5 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.6
		zhoneTrapPort OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This variable is sent with all traps. This contains the
				value of the port that originated the trap. If this is a
				system or card level trap (no port is associated with the trap),
				this value is 0."
			::= { zhoneTraps 6 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.7
		zhoneTrapSubPort OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This variable is sent with all traps. This contains the
				value of the subport that originated the trap. If this is a
				system or card level trap (no port is associated with the trap),
				this value is 0. It may also be 0 if not subports are associated
				with this physical port such as in the case of an ethernet port."
			::= { zhoneTraps 7 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.8
		zhoneTrapProviderId OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"Unique  service provider ID.  This value is used 
				throughout the management system to link objects with 
				customers or service providers."
			::= { zhoneTraps 8 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.9
		zhoneTrapText OBJECT-TYPE
			SYNTAX SnmpAdminString (SIZE (0..80))
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"Any text associated with this trap. NULL is allowed"
			::= { zhoneTraps 9 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.10
		zhoneTrapPortType OBJECT-TYPE
			SYNTAX IANAifType
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This is the If Type. It is used to uniquely identify a port."
			::= { zhoneTraps 10 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.11
		zhoneTrapPortTypeExtension OBJECT-TYPE
			SYNTAX ZhoneIfType
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This is the If Type Extension. It is used to uniquely identify a port."
			::= { zhoneTraps 11 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.12
		zhoneTrapAlarmAction OBJECT-TYPE
			SYNTAX INTEGER
				{
				set(1),
				clear(2),
				informational(3)
				}
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This is the alarm action type for products supporting the
				alarm manager functionality."
			::= { zhoneTraps 12 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.13
		zhoneTrapAlarmId OBJECT-TYPE
			SYNTAX ZhoneAlarmTypeId
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"For products supporting the alarm manager functionality,
				this is the alarm id a particular trap is setting or
				clearing."
			::= { zhoneTraps 13 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.14
		zhoneTrapAlarmSeverity OBJECT-TYPE
			SYNTAX ZhoneAlarmSeverity
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"For products supporting the alarm manager functionality,
				this is the alarm severity a particular trap is setting
				or clearing."
			::= { zhoneTraps 14 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.15
		zhoneTrapConfigSyncResetInterval OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"The ConfigSync reset check is run in 6 minute intervals.
				This object represents the interval number and starts
				with interval number 1 at system bootup and will reset
				to 1 whenever the sytem reboots."
			::= { zhoneTraps 15 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.16
		zhoneTrapConfigSyncResetCount OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"This count is the number of times the device has had to
				reset ConfigSync.  Resetting configsync on the device 
				consists of closing the FTP data and control sockets,
				setting the configsyncstatus to syncerror, cleaning up
				any pending configsync records, resetting the
				ConfigSync state machine to the initial state, and 
				throwing a ConfigSyncReset Trap."
			::= { zhoneTraps 16 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.17
		zhoneTrapConfigSyncResetLastFtpTime OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"Last time device sent a ConfigSync Ftp command to ZMS.
				Measured using the Unix time() function.
				The internal representation of time is the number of
				seconds elapsed since 00:00 Universal time on
				January 1, 1970 in the Gregorian calendar
				(Julian day 2440587.5), with time stored as a 32 bit
				signed integer (long in the original C implementation)."
			::= { zhoneTraps 17 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.18
		zhoneTrapConfigSyncResetCurrentTime OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS accessible-for-notify
			STATUS current
			DESCRIPTION
				"Current device time returned using Unix time() function.
				The internal representation of time is the number of
				seconds elapsed since 00:00 Universal time on
				January 1, 1970 in the Gregorian calendar
				(Julian day 2440587.5), with time stored as a 32 bit
				signed integer (long in the original C implementation)."
			::= { zhoneTraps 18 }

		
		-- 1.3.6.1.4.1.5504.3.1.2.19
		zhoneTrapUtcTime OBJECT-TYPE
