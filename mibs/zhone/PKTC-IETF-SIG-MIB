   PKTC-IETF-SIG-MIB DEFINITIONS ::= BEGIN 
    
   IMPORTS 
       MODULE-IDENTITY,  
       OBJECT-TYPE,  
       Integer32, 
       Unsigned32, 
       mib-2  
             FROM SNMPv2-SMI  
       InetAddressType, 
       InetAddress, 
       InetPortNumber  
             FROM INET-ADDRESS-MIB  
       TEXTUAL-CONVENTION,  
       RowStatus,  
       TruthValue  
             FROM SNMPv2-TC  
       OBJECT-GROUP,  
       MODULE-COMPLIANCE  
             FROM SNMPv2-CONF  
       SnmpAdminString  
             FROM SNMP-FRAMEWORK-MIB  
       ifIndex   
             FROM IF-MIB 
       Dscp 
             FROM DIFFSERV-DSCP-TC
       ietfDrafts                        
             FROM Zhone;
    
